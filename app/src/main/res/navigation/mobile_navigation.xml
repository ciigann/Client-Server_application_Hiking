<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_coordinates">

    <fragment
        android:id="@+id/nav_coordinates"
        android:name="com.example.hiking.ui.Coordinates.CoordinatesFragment"
        android:label="coordinates"
        tools:layout="@layout/fragment_coordinates" />

    <fragment
        android:id="@+id/nav_places"
        android:name="com.example.hiking.ui.Places.PlacesFragment"
        android:label="places"
        tools:layout="@layout/fragment_places" />

    <fragment
        android:id="@+id/nav_global_places"
        android:name="com.example.hiking.ui.GlobalPlaces.GlobalPlacesFragment"
        android:label="global_places"
