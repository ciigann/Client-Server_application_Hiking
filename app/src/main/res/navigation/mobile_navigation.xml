<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_coordinates">

    <fragment
        android:id="@+id/nav_account"
        android:name="com.example.hiking.ui.Account.AccountFragment"
        android:label="Аккаунт"
        tools:layout="@layout/fragment_account" />

    <fragment
        android:id="@+id/nav_coordinates"
        android:name="com.example.hiking.ui.Coordinates.CoordinatesFragment"
        android:label="Координаты"
        tools:layout="@layout/fragment_coordinates" />

    <fragment
        android:id="@+id/nav_places"
        android:name="com.example.hiking.ui.Places.PlacesFragment"
        android:label="Места"
        tools:layout="@layout/fragment_places" />

    <fragment
        android:id="@+id/nav_global_places"
        android:name="com.example.hiking.ui.GlobalPlaces.GlobalPlacesFragment"
        android:label="Места пользователей"
        tools:layout="@layout/fragment_global_places" />

    <fragment
        android:id="@+id/nav_tracker"
        android:name="com.example.hiking.ui.Tracker.TrackerFragment"
        android:label="Трекер"
        tools:layout="@layout/fragment_tracker" />
</navigation>
